<%= render 'shared/navbar' %>
<h1>Contact Us</h1>

<%= form_with url: "/thank_you", method: :get do |f| %>
<%= f.label :name %>
<%= f.text_field :name%>
<%= f.label :email %>
<%= f.text_field :email%>
<%= f.label :message %>
<%= f.text_field :message %>
<%= f.label :query, "Search for:"%>
<%= f.text_field :query%>
<%= f.submit "Submit"%>
<% end %>