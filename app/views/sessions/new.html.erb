<h3 class="mb-3 create-headline">Log In</h3>

<%= form_with url: session_path  do |form|%>
    <div class="mb-3">
        <%= form.label :email, class: "form-label"%>
        <%= form.text_field :email, class: "form-control"%>
    </div>
    <div class="mb-3">
        <%= form.label :password, class: "form-label" %>
        <%= form.password_field :password, class: "form-control"%>
    </div>
    <%= form.submit "Log In", class: "btn btn-info"%>
<% end %>
<div class="mb-3"></div>
<h5 class="mt-3 pt-3 create-headline">Do not have an account?</h5> 
<button class="btn btn-info my-3"><a href="/users/new">Sign Up</a></button>
